{
  "name": "assessment-front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=0.0.0.0",
    "start:hmr": "ng serve --hmr",
    "prebuild:preprod": "node build-time.js",
    "build:preprod": "ng build --configuration preprod && npm run sentry:sourcemap",
    "build:sandbox": "ng build --configuration sandbox",
    "prebuild": "node build-time.js",
    "build": "ng build --configuration production",
    "build:prod": "npm run build && npm run sentry:sourcemap",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test-ci": "ng test --karma-config=karma-ci.conf.js --no-progress",
    "test-ci-bis": "ng test --no-watch --no-progress --watch=false --code-coverage --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "storybook": "ng run assessment-front:storybook",
    "build-storybook": "ng run assessment-front:build-storybook",
    "compodoc:build": "compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -s",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org alto-w0 --project trainx-front ./dist && sentry-cli sourcemaps upload --org alto-w0 --project trainx-front ./dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.10",
    "@angular/common": "^15.2.10",
    "@angular/compiler": "^15.2.10",
    "@angular/core": "^15.2.10",
    "@angular/forms": "^15.2.10",
    "@angular/platform-browser": "^15.2.10",
    "@angular/platform-browser-dynamic": "^15.2.10",
    "@angular/router": "^15.2.10",
    "@angular/service-worker": "^15.2.10",
    "@auth0/auth0-angular": "^2.0.2",
    "@ng-bootstrap/ng-bootstrap": "^14.2.0",
    "@ng-select/ng-select": "^10.0.3",
    "@ngneat/until-destroy": "^9.2.3",
    "@ngrx/store": "15.2",
    "@popperjs/core": "^2.11.6",
    "@sentry/angular-ivy": "^7.93.0",
    "@sentry/cli": "^2.25.0",
    "@usealto/sdk-ts-angular": "^1.0.202312061105-dev",
    "bootstrap": "^5.3.0",
    "bootstrap-icons": "^1.10.5",
    "date-fns": "^2.29.3",
    "echarts": "^5.4.2",
    "memoizee": "^0.4.15",
    "ngx-echarts": "^15.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.10",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.2.10",
    "@angular/compiler-cli": "^15.2.10",
    "@angular/localize": "^15.2.10",
    "@compodoc/compodoc": "^1.1.21",
    "@cypress/schematic": "^2.5.0",
    "@ngrx/eslint-plugin": "^15.2.1",
    "@storybook/addon-docs": "^7.3.2",
    "@storybook/addon-essentials": "^7.3.2",
    "@storybook/addon-interactions": "^7.3.2",
    "@storybook/addon-links": "^7.3.2",
    "@storybook/angular": "^7.3.2",
    "@storybook/blocks": "^7.3.2",
    "@storybook/testing-library": "^0.2.0",
    "@types/chart.js": "^2.9.37",
    "@types/jasmine": "~4.3.0",
    "@types/memoizee": "^0.4.8",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "cypress": "latest",
    "eslint": "^8.33.0",
    "eslint-plugin-storybook": "^0.6.13",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "react": "^18.2.0",
